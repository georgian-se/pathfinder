wb = xlsx_package.workbook
wb.add_worksheet(name: "substations") do |sheet|
  sheet.add_row [ 'ID','kmlID','დასახელება','რეგიონი','განედი','გრძედი', 'აღწერილობა' ]
  @substations.each do |substation|
    sheet.add_row [
      substation.id.to_s,
      substation.kmlid,
      substation.name,
      substation.region && substation.region.name,
      substation.lat,
      substation.lng,
      substation.description
    ]
  end
end