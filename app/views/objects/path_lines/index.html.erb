<%=table_for @lines, title:@title, icon:'/icons/arrow-switch.png', collapsible:true do |t|
  t.item_action ->(x){objects_path_line_url(id:x.id)}, icon: '/icons/eye.png'
  t.text_field 'name', url: ->(x){objects_path_line_url(id:x.id)}
  t.text_field 'description'
  t.text_field 'points.count', tag:'code', i18n:'points'
  t.paginate records: 'ჩანაწერი'
end%>