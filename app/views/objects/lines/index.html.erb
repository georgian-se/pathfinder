<%=table_for @lines, title: @title, icon: '/icons/line.png', collapsible: true do |t|
  t.title_action objects_upload_lines_url, label: 'მონაცემების ატვირთვა', icon: '/icons/upload-cloud.png'
  t.item_action ->(x){objects_line_url(id:x.id)}, icon: '/icons/eye.png'
  t.text_field 'kmlid', tag: 'code'
  t.text_field 'name', url: ->(x){objects_line_url(id:x.id)}
  t.text_field 'region.name', i18n: 'region'
  t.text_field 'points.count', i18n: 'points', tag: 'code'
  t.paginate records: 'ჩანაწერი'
end%>